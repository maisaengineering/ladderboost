- @activities.each do |activity|
  - activitys =  activity.attributes.reject {|k,v| k if k.start_with?('key','parameters','_id','updated_at','trackable_id', 'created_at', 'trackable_type')}
  -activitys.each do |updated_field|
  - user_image = User.where('profile._id' => activity.trackable_id).first
    %table
      %tr
        %td.activity_users
          = user_image.profile.first_name
      %tr
        %td.activity_users
          = updated_field[0].gsub(/_/, ' ').capitalize + " " + "updated to" + " " + updated_field[1]